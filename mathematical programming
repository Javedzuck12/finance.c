//to check the condition as age
#include<stdio.h>
int main()
{
    int age;
    printf("enter number :\n");
    scanf("%d",&age);

    if(age>=18){
        printf("you are adult\n");
    }
    else{
        printf("you are not\n");
    }
    return 0;
}
