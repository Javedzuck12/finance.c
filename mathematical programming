}



//switch case on the basis week
#include<stdio.h>
int main()
{
    int day;
    printf("Enter the number:\n");
    scanf("%d",&day);

    switch(day){

    case 1:printf("monday\n");
     break;

    case 2:printf("tuesday\n");
     break;

    case 3:printf("wednesday\n");
     break;

    case 4:printf("thursday\n");
     break;

    case 5:printf("friday\n");
     break;

    case 6:printf("saturday\n");
     break;

    case 7:printf("sunday\n");
     break;
    }
    return 0;
}

//alphabet uses in switch
#include<stdio.h>
int main()
{
    char day;
    printf("Enter the character:\n");
    scanf("%ch",&day);

    switch(day){

    case 'm':printf("Monday\n");
     break;

    case 't':printf("Tuesday\n");
     break;

    case 'w':printf("Wednesday\n");
     break;

    case 'T':printf("Thursday\n");
     break;

    case 'f':printf("Friday\n");
     break;

    case 's':printf("Saturday\n");
     break;

    case 'S':printf("Sunday\n");
     break;
    }
     return 0;
}

#include<stdio.h>
int main()
{
    int num=12;
    int letter=12;
    printf("enter the letter:\n");
    scanf("%d",&letter);

    num==letter ? printf("you are right\n"):printf("you are not\n");
    return 0;

}

//loop
#include<stdio.h>
int main()
{
    for(int i=1;i<=10;i--){
        printf("%d\n",i);
    }
}
#include <stdio.h>
int calcsquare(int n);

    int main(){
    int n;
    printf("enter the number:\n");
    scanf("%d",n);
    printf("the square  %d",calcsquare(n));
    return 0;
    }

    int calcsquare(int n){
    return n*n;
    }


//loop
#include<stdio.h>
int main()
{
    int i=1;
    while(i<=19){
        printf("%d\n",i);
        i++;
    }
    return 0;
}

//loop
#include<stdio.h>
int main()
{
    int i=1;
    do {
        printf("%d\n",i);
        i++;
        }while(i<=10);
        return 0;
}

//to program the n of their number and their sum
#include<stdio.h>
int main()
{
    int i,n,sum=0;
    printf("test the number:\n");
    scanf("%d",&n);
    printf("The first n natural number:\n%d",&n);
    for(int i=1;i<=n;i++){
        printf("%d",i);
    }
    for(int i=1;i<=n;i++){
        sum=sum+i;
    }
    printf("The sum of their natural number:%d",sum);
    return 0;
}

