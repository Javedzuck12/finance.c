// This is a program to display the multiplication table of a given integer.
#include<stdio.h>
int main()
{ int i,num;
printf("Input the number(Table to be calculated):\n");
scanf("%d",&num);
for(i=1;i<=10;i++)
{ printf("%d * %d = %d\n",num,i,num*i);
}
return 0;}

//to display the factorial
#include<stdio.h>
int main()
{int i,fact=1,num;
printf("Input the number : ");
scanf("%d",&num);
for(i=1;i<=num;i++)
{ fact=fact*i;
}
printf("Factorial of %d is : %d",num,fact);
return 0;
}
OUTPUT
#include <stdio.h>
int main()
{
    int marks;
    printf("enter your marks:\n");
    scanf("%d",&marks);
    if(marks>=60){
        printf("you are first division\n");
    }
    else if(marks<=60){
        printf("you are average\n");
    }
    else{
        printf("you are not pass \n");
    }

    return 0;
}

//to program to calculate the time of circular motion
#include <stdio.h>
int main()
{
    int radius,velocity;
    printf("Enter the radius:\n");
    scanf("%d",&radius);
    printf("Enter the velocity:\n");
    scanf("%d",&velocity);
     printf("The time period of circular object is :%f",2*3.14*radius/velocity);
     return 0;
}
