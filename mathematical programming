    #include<stdio.h>
    int main()
    {
        float radius;
        printf("Enter the radius:");
        scanf("%f",&radius);
        printf("The area of circle is:%f\",3.14*radius*radius);
        return 0;

    }
 //self thought program auto increase number
   #include<stdio.h>
int main()
{
    int age;
    int oldage=age;
    int newage=oldage+2;
    printf("enter the age\n:");
    scanf("%d",&age);
    printf("the new age is %d",newage);
    return 0;

}


