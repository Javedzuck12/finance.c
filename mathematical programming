#include<stdio.h>
int main()
{
    int a, b;
    printf("enter first number \n");
    scanf("%d",&a);
    printf("enter a second number \n");
    scanf("%d",&b);
    printf("sum of two number is %d \n",a+b);
    return 0;
}

